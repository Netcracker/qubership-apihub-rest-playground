{
  "name": "@netcracker/qubership-apihub-rest-playground",
  "version": "1.2.1",
  "description": "Web Component for Try It",
  "sideEffects": [
    "./dist/index.js",
    "./dist/hooks.js"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "types": "./typings/index.d.ts",
  "exports": {
    "./index.css": "./dist/styles.min.css",
    ".": "./dist/index.js",
    "./hooks": {
      "types": "./dist/src/hooks/index.d.ts",
      "default": "./dist/hooks.js"
    }
  },
  "typesVersions": {
    ">=4.7": {
      "hooks": [
        "./dist/src/hooks/index.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=14.13"
  },
  "scripts": {
    "build": "npm run build.webcomponents && npm run build.hooks && npm run build.styles",
    "build.webcomponents": "webpack -c webpack.web-components.config.js",
    "build.hooks": "webpack -c ./webpack.hooks.config.js",
    "build.styles": "postcss src/styles.css -o dist/styles.min.css",
    "build:showcase": "storybook build -o ./dist-showcase",
    "test": "jest -c .jest/unit-tests/unit-test.jest.config.js",
    "lint": "eslint . --ext .ts,.tsx,.json,.jsonc --ignore-path .gitignore --max-warnings 0 --report-unused-disable-directives",
    "storybook": "storybook dev -p 9001",
    "development:link": "echo please run \"npm link\" manually in console to avoid using the deprecated node_modules/.bin/npm as it will reinstall all dependencies",
    "development:unlink": "echo please run \"npm unlink\" manually in console to avoid using the deprecated node_modules/.bin/npm as it will reinstall all dependencies",
    "update-lock-file": "update-lock-file @netcracker"
  },
  "rollup": {
    "bundleDeps": [
      "@fortawesome/free-solid-svg-icons"
    ]
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^15.0.0",
